{
  "name": "safetap-backoffice",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "prisma generate --schema=prisma/schema.prisma && next build",
    "build:deploy": "prisma generate --schema=prisma/schema.prisma && prisma migrate deploy --schema=prisma/schema.prisma && next build",
    "build:vercel": "prisma generate --schema=prisma/schema.prisma --no-engine && next build",
    "next:build": "next build",
    "start": "next start -p 3001",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:ci": "vitest run --reporter=verbose",
    "test:watch": "vitest",
    "postinstall": "prisma generate --schema=prisma/schema.prisma",
    "check-all": "npm run type-check && npm run lint && npm run format:check && npm run test",
    "analyze": "ANALYZE=true npm run build",
    "analyze:server": "BUNDLE_ANALYZE=server npm run build",
    "analyze:browser": "BUNDLE_ANALYZE=browser npm run build"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@hookform/resolvers": "^3.9.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.14.0",
    "@prisma/extension-accelerate": "^1.2.1",
    "@types/nodemailer": "^7.0.0",
    "@types/qrcode": "^1.5.5",
    "autoprefixer": "^10.4.19",
    "chart.js": "^4.4.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "html-to-image": "^1.11.13",
    "html2canvas-pro": "^1.5.11",
    "lucide-react": "^0.446.0",
    "next": "^15.5.0",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.10.1",
    "postcss": "^8.4.38",
    "qrcode": "^1.5.4",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.2.0",
    "react-hook-form": "^7.53.0",
    "recharts": "^2.12.7",
    "tailwindcss": "^3.4.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.17.0",
    "eslint-config-next": "^15.5.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "prisma": "^6.14.0",
    "typescript": "^5",
    "vitest": "^3.2.4",
    "webpack-bundle-analyzer": "^4.10.1"
  }
}
